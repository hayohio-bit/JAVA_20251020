package test;

public class Sample {

	public static void main(String[] args) {
		
		System.out.println("Hello, Jave");
		System.out.println("Hello, Jave");

		char var1 = 'A';
		char c1 = 65;
		
		char var2 = '가';
//		char c2 = 44032;
		char c2 = 0xac63;
		
		System.out.println(var1);
		System.out.println(c1);
		System.out.println(var2);
		System.out.println(c2);
		
		System.out.printf("Hello %s!", "World");
	
		
		float f1 = 10.5f;
		//컴파일러는 실수 리터럴을 기본적으로 double 타입으로 해석하므로 float 타입에 대입하고 싶다면 리터럴 뒤에 F 붙임
		double d1 = 10.5;
		System.out.println(f1);
		System.out.println(d1);
	
		float f2 = 2147483648f;
		float f3 = 2147483658f;
		float f4 = 2147483668f;
		
		System.out.println(f2);
		System.out.println(f3);
		System.out.println(f4);
		
		//2.1474836E9 2.1474836x 10 -> 2,147,483,648
		
		String str1 = "김대철";
		String str2 = "홍\"길\\동\t";
		String str3 = "홍\n길\r동\u0061"; //유니코드 \u ~
		
		System.out.println(str1);
		System.out.println(str2);
		System.out.println(str3);
		System.out.println("\r" + "캐리지" + "\r" + "리턴");
		System.out.println("\r캐리지\r리턴");
		System.out.println("aa\rbb\ncc");
		
		
		//자동 형변환
		int intA = 10000;
		byte byteB = 20;
		
		//intA = byteB; // a = (int)b;
		
		System.out.println(intA);
		System.out.println(byteB);
		
		//강제 형변환
		System.out.println("------강제 형변환-------------");
		byteB = (byte)intA;
		
		System.out.println(intA);
		System.out.println(byteB);
		
		
		
	}

}
